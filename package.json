{
	"name": "cnfgrble",
	"version": "0.8.0",
	"description": "Checks if property is configurable.",
	"main": "cnfgrble.js",
	"support": "cnfgrble.support.js",
	"deploy": "cnfgrble.deploy.js",
	"browser": "cnfgrble.deploy.js",
	"scripts": {
		"pristine": "rsetmod clean && flxc execute ./.install.sh",
		"clear": "rsetmod clean --clear",
		"build": "babel cnfgrble.js --out-file cnfgrble.support.js --source-maps && webpack --progress --display-modules",
		"pretest": "npm run clear && npm run build",
		"test": "node cnfgrble-test.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/volkovasystems/cnfgrble.git"
	},
	"keywords": [
		"check",
		"property",
		"configurable"
	],
	"author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
	"contributors": [
		"John Lenon Maghanoy <johnlenonmaghanoy@gmail.com>",
		"Vinse Vinalon <vinsevinalon@gmail.com>"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/volkovasystems/cnfgrble/issues"
	},
	"homepage": "https://github.com/volkovasystems/cnfgrble#readme",
	"dependencies": {
		"asyum": "^0.9.0",
		"babel-runtime": "^6.25.0",
		"dscrb": "^0.7.0",
		"zelf": "^0.14.0"
	},
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-core": "^6.25.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-latest": "^6.24.1",
		"source-map-loader": "^0.2.1",
		"webpack": "^3.5.2"
	}
}
